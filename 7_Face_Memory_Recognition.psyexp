    <Routine name="inCode">
      <CodeComponent name="codeIn">
        <Param name="Begin Experiment" updates="constant" val="import random, xlrd&amp;#10;&amp;#10;#randomize the seed&amp;#10;random.seed()&amp;#10;&amp;#10;#stimulus file&amp;#10;infile1 = '6_stimuli_face_memory_recognition.xlsx'&amp;#10;&amp;#10;#number of study items&amp;#10;num_study1 = 30&amp;#10;&amp;#10;#number of test items&amp;#10;num_test1 = 60&amp;#10;&amp;#10;#total number of items&amp;#10;num_items1 = 60" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="import * as random from 'random';&amp;#10;import * as xlrd from 'xlrd';&amp;#10;var infile1, num_items1, num_study1, num_test1;&amp;#10;random.seed();&amp;#10;infile1 = &quot;6_stimuli_face_memory_recognition.xlsx&quot;;&amp;#10;num_study1 = 30;&amp;#10;num_test1 = 60;&amp;#10;num_items1 = 60;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#access the stimulus file&amp;#10;inbook1 = xlrd.open_workbook(infile1)&amp;#10;insheet1 = inbook1.sheet_by_index(0)&amp;#10;&amp;#10;#arrays for our stimuli&amp;#10;word1 = []&amp;#10;&amp;#10;#read the stimuli from our sheet&amp;#10;for rowx1 in range(1,num_items1+1):&amp;#10;    &amp;#10;    #read in the values of all columns on this row&amp;#10;    row1 = insheet1.row_values(rowx1)&amp;#10;    &amp;#10;    #print out the value of column A for this row&amp;#10;    #print row1[0]&amp;#10;&amp;#10;    #saving the word to the word array&amp;#10;    word1.append(row1[0])&amp;#10;&amp;#10;#randomize the order of the word array&amp;#10;random.shuffle(word1)&amp;#10;&amp;#10;#arrays for our final array&amp;#10;word_item1 =[]&amp;#10;old_new1 = [] &amp;#10;&amp;#10;#create the final stimulus list&amp;#10;for i1 in range(num_test1):&amp;#10;&amp;#10;    #assign a word item from the word list&amp;#10;    word_item1.append(word1[i1])&amp;#10;&amp;#10;    #determine the old/new status of items&amp;#10;    if i1&lt;30:&amp;#10;        old_new1.append('old')&amp;#10;    else:&amp;#10;        old_new1.append('new')&amp;#10;&amp;#10;    #user output&amp;#10;    #print 'item number '+str(i1)+' '+word_item1[i1]+' '+old_new1[i1]" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="codeIn" valType="code"/>
      </CodeComponent>
    </Routine>
    <Routine name="TestTCode">
